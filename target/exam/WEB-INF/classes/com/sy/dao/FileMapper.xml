        SELECT
        <include refid="Base_Column_List"/>
        FROM t_file
        <where>
            <if test="id != null ">
                and id= #{id}
            </if>
            <if test="fType != null and fType != ''">
                and f_type= #{fType}
            </if>
            <if test="fileName != null and fileName != ''">
                and file_name like concat('%',#{fileName},'%')
            </if>
        </where>
    </select>

    <select id="selectByFilename" resultType="com.sy.entity.other.KeyValue">
        SELECT  id as value,file_name as name
		from t_file
		where file_name like concat('%',#{value},'%')
		limit 5
